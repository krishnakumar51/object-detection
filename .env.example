# NGROK CONFIGURATION
# Sign up at https://ngrok.com and get your auth token
NGROK_AUTHTOKEN=<auth-token>
USE_NGROK=true
NEXT_PUBLIC_USE_NGROK=true

# APPLICATION SETTINGS
MODE=wasm
NODE_ENV=production

# LEAVE ALL THESE EMPTY - AUTO-DISCOVERY WILL FIND THEM
NEXT_PUBLIC_SIGNALING_SERVER_URL=
NEXT_PUBLIC_BASE_URL=
NEXT_PUBLIC_NGROK_URL=

# Optional: Database or other service URLs (if any)
# DATABASE_URL=your_database_url_here
# REDIS_URL=your_redis_url_here